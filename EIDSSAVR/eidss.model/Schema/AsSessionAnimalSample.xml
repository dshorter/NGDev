          <lambda_handler field="idfAnimal" target="AnimalAge" type="AnimalAgeLookup" lambda="c => c.AnimalsLookup.FirstOrDefault(i => i.idfAnimal == c.idfAnimal, i => i.AnimalAge)"/>
