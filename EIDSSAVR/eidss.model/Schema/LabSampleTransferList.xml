				<item name="TransferTo"  />
			</grid>

			
			
			<labels>
				<item name="strBarcode" labelId="LabSampleTransferListItem.strBarcode"/>
			</labels>
			
			<storage>
				<delete name="spReadOnlyObject_Delete"/>
