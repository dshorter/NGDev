<?xml version="1.0" encoding="utf-8" ?>
<object name="CollectionMethodLookup"
        connection="EidssConnectionString"
        generator="ObjectGenerator.xslt"
        xmlns="urn:schemas-bv:objectmodel">

    <storage>
        <get name="spCollectionMethod_SelectLookup" type="lookuplist" cache="true"/>
    </storage>

    <tables>
        <table name="CollectionMethodLookup" tostring="c => c.CMName" lookupcachename="CollectionMethod">
          <keys>
            <key name="idfsCollectionMethod"/>
          </keys>
        </table>
    </tables>

</object>

