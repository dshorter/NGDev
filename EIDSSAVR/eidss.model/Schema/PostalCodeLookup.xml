<?xml version="1.0" encoding="utf-8" ?>
<object name="PostalCodeLookup"
        connection="EidssConnectionString"
        generator="ObjectGenerator.xslt"
        xmlns="urn:schemas-bv:objectmodel">

    <storage>
        <get name="spPostalCode_SelectLookup" type="lookuplist"/>
    </storage>

    <tables>
        <table name="PostalCodeLookup" tostring="c => c.strPostCode" lookupcachename="PostalCode">
            <keys>
                <key name="strPostCode"/>
            </keys>
        </table>
    </tables>

</object>

