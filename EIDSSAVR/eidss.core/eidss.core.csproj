  <Target Name="AfterBuild" DependsOnTargets="$(AfterBuildDependsOn)" />
  <Target Name="CopyGISData">
    <ItemGroup>
      <_gisDlls Include="$(SolutionDir)dlls\gis\*.*" />
      <_gdalData Include="$(SolutionDir)dlls\gis\Data\**\*" />
      <_esri Include="$(SolutionDir)dlls\gis\ESRI\**\*" />
    </ItemGroup>
    <Copy SourceFiles="@(_gisDlls)" DestinationFolder="$(TargetDir)">
      <Output TaskParameter="CopiedFiles" ItemName="FileWrites" />
    </Copy>
    <PropertyGroup>
      <_targetGdalData>$(TargetDir)gdal_data\</_targetGdalData>
    </PropertyGroup>
    <RemoveDir Directories="$(_targetGdalData)" />
    <MakeDir Directories="$(_targetGdalData)" />
    <Copy SourceFiles="@(_gdalData)" DestinationFolder="$(_targetGdalData)">
      <Output TaskParameter="CopiedFiles" ItemName="FileWrites" />
    </Copy>
    <PropertyGroup>
      <_targetESRI>$(TargetDir)ESRI\</_targetESRI>
    </PropertyGroup>
    <RemoveDir Directories="$(_targetESRI)" />
    <MakeDir Directories="$(_targetESRI)" />
    <Copy SourceFiles="@(_esri)" DestinationFiles="@(_esri->'$(_targetESRI)%(RecursiveDir)%(Filename)%(Extension)')">
      <Output TaskParameter="CopiedFiles" ItemName="FileWrites" />
    </Copy>
  </Target>
