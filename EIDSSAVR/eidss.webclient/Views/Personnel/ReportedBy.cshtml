@using eidss.model.Schema
@using eidss.web.common.Utils
@using eidss.webclient.Utils
@model eidss.model.Schema.VetCase

<div class="sectionTitle">
    @Translator.GetMessageString("titleSectionReported")
</div>

@Html.Action("SelectPerson", "Personnel", new { vcase = Model, idfsOfficePropertyName = "idfReportedByOffice", strOfficePropertyName = "strReportedByOffice",
                    idfsPersonPropertyName = "idfPersonReportedBy", strPersonPropertyName = "strPersonReportedBy", showEditPersonButton = true, HACode = (Model as VetCase)._HACode })

<table class="level1">
    <tr>
        <td class="xlarge firstLabel">
            @Html.BvLabel(Model, m => m.datReportDate)
        </td>
        <td class="large">
            @Html.BvDatebox(Model, m => m.datReportDate)
        </td>
    </tr>
</table>
