@{
    ViewBag.Title = @Translator.GetMessageString("MainMenu");
    Layout = "~/Views/Shared/_SimpleLayout.cshtml";
}

@using eidss.mobileclient.Utils

@if (ViewBag.IsHumanModuleVisible)
{
    <br />
    <input type="submit" class="fillAll" value="@Translator.GetMessageString("titleHumanModule")" onclick="document.location.href='@Url.Action("HumanModule", "HumanCase")';" />
    <br />
}
@if (ViewBag.IsVetModuleVisible)
{
    <br /> 
    <input type="submit" class="fillAll" value="@Translator.GetMessageString("titleVetModule")" onclick="document.location.href='@Url.Action("VetModule", "VetCase")';" /> 
}