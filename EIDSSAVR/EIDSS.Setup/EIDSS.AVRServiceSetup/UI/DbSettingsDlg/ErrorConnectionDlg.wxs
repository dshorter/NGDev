<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
  <Fragment>
    <UI>
      <Dialog Id="ErrorConnectionDlg" Width="230" Height="80" Title="!(loc.ErrorDlg_Title)">
        <Control Id="TestResult_Failure" Type="Icon" IconSize="48" X="5" Y="5" Width="50" Height="50" Text="FailureImg" FixedSize="yes" />
        <Control Id="ErrorText" Type="Text" X="60" Y="15" Width="160" Height="40" TabSkip="no" Text="[ErrorConnectionText]" />
        <Control Id="O" Type="PushButton" X="100" Y="60" Width="56" Height="17" TabSkip="yes" Text="!(loc.WixUIOK)">
          <Publish Event="EndDialog" Value="Return">1</Publish>
        </Control>
      </Dialog>
    </UI>

    <Property Id="ErrorConnectionText_EmptyMandatoryFileds" Value="!(loc.ErrorConnectionText_EmptyMandatoryFileds)" />
  </Fragment>
</Wix>