<?xml version="1.0" encoding="utf-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
  <Fragment>
    <Property Id="DeployType" Value="InstallAvrDb" />

    <UI>
      <Dialog Id="SelectAvrDbDeploymentTypeDlg" Width="370" Height="270" Title="!(loc.SelectAvrDbDeploymentTypeDlg_Title)">
        <Control Id="Next" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="!(loc.WixUINext)">
          <Publish Property="ADMIN_AVR_PASSWORD" Value="{}" Order="1">1</Publish>
        </Control>
        <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Text="!(loc.WixUIBack)" />
        <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="!(loc.WixUICancel)">
          <Publish Event="SpawnDialog" Value="CancelDlg">1</Publish>
        </Control>
        <Control Id="Description" Type="Text" X="25" Y="23" Width="300" Height="15" Transparent="yes" NoPrefix="yes" Text="!(loc.SelectAvrDbDeploymentTypeDlgDescription)" />
        <Control Id="Title" Type="Text" X="15" Y="6" Width="250" Height="15" Transparent="yes" NoPrefix="yes" Text="!(loc.SelectAvrDbDeploymentTypeDlgTitle)" />
        <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="!(loc.SelectAvrDbDeploymentTypeDlgBannerBitmap)" />
        <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />
        <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />

        <Control Id="DeploymentTypeGroup" Type="RadioButtonGroup" X="30" Y="94" Width="305" Height="100" Property="DeployType">
          <RadioButtonGroup Property="DeployType">
            <RadioButton Value="InstallAvrDb" X="0" Y="0" Width="200" Height="10" Text="!(loc.DeploymentTypeInstall)" />
            <RadioButton Value="FindAvrDb" X="0" Y="20" Width="200" Height="10" Text="!(loc.DeploymentTypeFind)" />
          </RadioButtonGroup>
        </Control>
      </Dialog>
    </UI>
  </Fragment>
</Wix>