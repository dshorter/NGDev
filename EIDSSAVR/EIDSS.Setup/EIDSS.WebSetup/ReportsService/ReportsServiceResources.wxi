<?ifdef force_all_resources?>
<?ifdef no_bv.common.resources.dll?>
<?undef no_bv.common.resources.dll?>
<?endif?>
<?ifdef no_bv.winclient.resources.dll?>
<?undef no_bv.winclient.resources.dll?>
<?endif?>
<?ifdef no_bvwin_common.resources.dll?>
