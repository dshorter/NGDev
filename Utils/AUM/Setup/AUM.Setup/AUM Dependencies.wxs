  <Fragment Id="Dependencies">
    <ComponentGroup Id="AVR.Dependencies">
      <ComponentGroupRef Id="Zip" />
      <ComponentGroupRef Id="SqlServer.Harvested" />
    </ComponentGroup>
  </Fragment>

  <Fragment Id="Zip">
