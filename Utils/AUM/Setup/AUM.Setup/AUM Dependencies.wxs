      </Component>
      <Component Id="SevenZipSharp.dll">
        <File Name="SevenZipSharp.dll" KeyPath="yes" Checksum="yes" />
      </Component>
      <Component Id="_7_zip.dll">
        <File Name="7-zip.dll" KeyPath="yes" Checksum="yes" />
      </Component>
      <Component Id="_7z.dll">
        <File Name="7z.dll" KeyPath="yes" Checksum="yes" />
      </Component>
    </ComponentGroup>
  </Fragment>
</Wix>