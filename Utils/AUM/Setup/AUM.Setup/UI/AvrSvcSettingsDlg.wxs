<?xml version="1.0" encoding="utf-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
  <Fragment>
    <PropertyRef Id="AVR_SERVICE" />
    <UI>
      <Dialog Id="AvrSvcSettingsDlg" Width="370" Height="270" Title="!(loc.AvrSvcSettingsDlg_Title)">
        <Control Id="Next" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="!(loc.WixUINext)" />
        <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Text="!(loc.WixUIBack)" />
        <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="!(loc.WixUICancel)">
          <Publish Event="SpawnDialog" Value="CancelDlg">1</Publish>
        </Control>
        <Control Id="Description" Type="Text" X="25" Y="23" Width="280" Height="15" Transparent="yes" NoPrefix="yes" Text="!(loc.AvrSvcSettingsDlgDescription)" />
        <Control Id="Title" Type="Text" X="15" Y="6" Width="200" Height="15" Transparent="yes" NoPrefix="yes" Text="!(loc.AvrSvcSettingsDlgTitle)" />
        <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="!(loc.AvrSvcSettingsDlgBannerBitmap)" />
        <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />
        <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />

        <Control Id="ServiceSelectorTitle" Type="Text" Width="80" Height="17" X="50" Y="110" Text="!(loc.AvrSvcSettingsServiceSelectorTitle)" />
        <Control Id="ServiceNames" Type="ComboBox" Width="260" Height="16" X="50" Y="130" Property="AVR_SERVICE" ComboList="yes" Sorted="yes">
          <ComboBox Property="AVR_SERVICE">
            <ListItem Text="!(loc.AvrSvcSettingsDlgNotPresentText)" Value="!(loc.AvrSvcSettingsDlgNotPresentValue)" />
          </ComboBox>
        </Control>
      </Dialog>
    </UI>

    <Property Id="AvrServiceNotPresentText" Value="!(loc.AvrSvcSettingsDlgNotPresentText)" />
    <Property Id="AvrServiceNotPresentValue" Value="!(loc.AvrSvcSettingsDlgNotPresentValue)" />
  </Fragment>
</Wix>