<?xml version="1.0" encoding="UTF-8"?>

<?include "ProductInfo.wxi" ?>

<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
  <Fragment Id="Directory tree">
    <Directory Id="TARGETDIR" Name="SourceDir">
      <Directory Id="ProgramMenuFolder" SourceName="Programs">
        <Directory Id="BVShortcutFolder" Name="$(var.Manufacturer)">
          <Directory Id="EIDSSShortcutFolder" Name="$(var.ProductFullName)" />
        </Directory>
      </Directory>

      <Directory Id="SystemDrive" ComponentGuidGenerationSeed="{301C1BCF-F4D1-4B07-BD26-A381A7660403}">
        <Directory Id="ApplicationFolder" Name="$(var.ProductNameAbbreviation)$(var.MajorVersion)$(var.FuzzyMinorVersion)">
          <Directory Id="AZ_LATN_AZ" Name="az-Latn-AZ" />
          <Directory Id="KA_GE" Name="ka-GE" />
          <Directory Id="KK_KZ" Name="kk-KZ" />
          <Directory Id="RU_RU" Name="ru-RU" />
          <Directory Id="UK_UA" Name="uk-UA" />
          <Directory Id="AUMupdate" Name="AUMupdate" />
          <Directory Id="AUMlog" Name="AUMlog" />
          <Directory Id="AUMlogBackup" Name="AUMlogBackup" />
          <Directory Id="BACKUPS" Name="backups" />
          <Directory Id="temp" Name="temp" />
        </Directory>
      </Directory>
    </Directory>

    <SetDirectory Id="ApplicationFolder" Value="[%SystemDrive]\$(var.ProductNameAbbreviation)$(var.MajorVersion)$(var.FuzzyMinorVersion)\" />
  </Fragment>
</Wix>